{
   "background": {
      "persistent": false,
      "scripts": [ "js/tsBackground.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "css/tsStyle.css" ],
      "js": [ "js/jquery.min.js", "js/tsInject.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "Enables Click to Dial functionality along side your Hosted PBX.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Click to Dial - Hosted PBX",
   "permissions": [ "storage", "http://*/*", "https://*/*", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0"
}
